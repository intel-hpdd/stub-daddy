{
  "name": "intel-stub-daddy",
  "version": "1.0.7",
  "description": "Stub API service used for integration testing.",
  "main": "index.js",
  "scripts": {
    "test": "JASMINE_CONFIG_PATH=jasmine.json NODE_ENV=test jasmine",
    "unit": "JASMINE_CONFIG_PATH=jasmine.json NODE_ENV=test jasmine test/unit/**/*.js",
    "integration": "JASMINE_CONFIG_PATH=jasmine.json NODE_ENV=test jasmine test/integration/**/*.js",
    "jshint": "jshint --c ./.jshintrc --exclude-path ./.jshintignore .",
    "jscs": "jscs ."
  },
  "pre-commit": [
    "jscs",
    "jshint",
    "test"
  ],
  "repository" : {
    "type" : "git",
    "url": "https://github.intel.com/IML/stub-daddy.git"
  },
  "publishConfig": {
    "registry": "https://ubit-artifactory-or.intel.com/artifactory/api/npm/iml-npm-local"
  },
  "author": "IML Team",
  "license": "MIT",
  "dependencies": {
    "bunyan": "1.3.5",
    "configulator": "1.0.2",
    "jsonschema": "0.4.0",
    "wiretree": "0.2.5",
    "yargs": "1.2.6",
    "promise": "5.0.0",
    "intel-lodash-mixins": "1.0.2"
  },
  "devDependencies": {
    "dotty": "0.0.2",
    "request-then": "0.0.1",
    "jscs": "2.5.1",
    "jshint": "2.8.0",
    "pre-commit": "1.1.2",
    "jasmine": "2.3.2",
    "krusty-jasmine-reporter": "1.0.0"
  }
}
