{
  "name": "@intel-js/stub-daddy",
  "version": "1.0.4",
  "description": "Stub API service used for integration testing.",
  "main": "index.js",
  "scripts": {
    "test": "JASMINE_CONFIG_PATH=jasmine.json NODE_ENV=test jasmine",
    "unit": "JASMINE_CONFIG_PATH=jasmine.json NODE_ENV=test jasmine test/unit/**/*.js",
    "integration": "JASMINE_CONFIG_PATH=jasmine.json NODE_ENV=test jasmine test/integration/**/*.js",
    "jshint": "jshint --c ./.jshintrc --exclude-path ./.jshintignore .",
    "jscs": "jscs ."
  },
  "pre-commit": [
    "jscs",
    "jshint",
    "test"
  ],
  "repository" : {
    "type" : "git",
    "url" : "https://github.com/intel-js/stub-daddy.git"
  },
  "author": "IML Team",
  "license": "MIT",
  "dependencies": {
    "bunyan": "1.3.5",
    "configulator": "1.0.2",
    "jsonschema": "0.4.0",
    "wiretree": "0.2.5",
    "yargs": "1.2.6",
    "promise": "5.0.0",
    "@intel-js/lodash-mixins": "1.0.0"
  },
  "devDependencies": {
    "dotty": "0.0.2",
    "request-then": "0.0.1",
    "jscs": "2.1.0",
    "jshint": "2.8.0",
    "pre-commit": "1.1.1",
    "jasmine": "2.3.2",
    "krusty-jasmine-reporter": "1.0.0"
  },
  "whitelist": ["dotty", "jasmine", "krusty-jasmine-reporter", "request-then"]
}
