{
  "name": "@mfl/stub-daddy",
  "version": "2.0.2",
  "description": "Stub API service used for integration testing.",
  "main": "server.js",
  "scripts": {
    "test": "jest",
    "debug": "node --debug-brk ./node_modules/.bin/jest --runInBand",
    "eslint": "eslint ./"
  },
  "pre-commit": [
    "eslint",
    "test"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:intel-hpdd/stub-daddy.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "author": "MFL Team",
  "license": "MIT",
  "jest": {
    "resetMocks": true,
    "resetModules": true,
    "clearMocks": true,
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/test"
    ],
    "transformIgnorePatterns": [
      "/node_modules/(?!@mfl)/"
    ]
  },
  "dependencies": {
    "@mfl/fp": "8.0.1",
    "@mfl/obj": "6.0.1",
    "@mfl/router": "6.0.1",
    "bunyan": "1.8.10",
    "deep-equal": "1.0.1",
    "jsonschema": "^1.1.1",
    "nconf": "0.8.4"
  },
  "devDependencies": {
    "@mfl/jasmine-n-matchers": "2.1.1",
    "@mfl/req": "3.0.4",
    "babel-eslint": "7.2.3",
    "babel-jest": "19.0.0",
    "babel-plugin-external-helpers": "6.22.0",
    "babel-plugin-transform-flow-strip-types": "6.22.0",
    "babel-plugin-transform-object-rest-spread": "6.23.0",
    "babel-preset-env": "1.4.0",
    "dotty": "0.0.2",
    "eslint": "3.19.0",
    "eslint-config-prettier": "1.7.0",
    "eslint-plugin-flowtype": "2.32.1",
    "eslint-plugin-import": "2.2.0",
    "eslint-plugin-prettier": "2.0.1",
    "jasmine": "^2.6.0",
    "jest": "^19.0.2",
    "pre-commit": "1.2.2",
    "prettier": "1.3.1"
  }
}
