{
  "name": "@iml/stub-daddy",
  "version": "2.0.5",
  "description": "Stub API service used for integration testing.",
  "main": "dist/bundle.js",
  "scripts": {
    "test": "jest",
    "debug": "node --debug-brk ./node_modules/.bin/jest --runInBand",
    "cover": "NODE_ENV=test yarn test -- --coverage",
    "eslint": "eslint ./",
    "postversion": "rm -rf ./dist; mkdir ./dist; cp cert.pem ./dist; cp key.pem ./dist; cp conf.json ./dist; webpack"
  },
  "pre-commit": ["eslint", "test"],
  "repository": {
    "type": "git",
    "url": "git@github.com:intel-hpdd/stub-daddy.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "author": "IML Team",
  "license": "MIT",
  "jest": {
    "resetMocks": true,
    "resetModules": true,
    "clearMocks": true,
    "coveragePathIgnorePatterns": ["/node_modules/", "/test"],
    "transformIgnorePatterns": ["/node_modules/(?!@iml)/"]
  },
  "dependencies": {
    "@iml/fp": "8.0.3",
    "@iml/obj": "6.0.3",
    "@iml/router": "6.0.2",
    "bunyan": "^1.8.12",
    "deep-equal": "1.0.1",
    "jsonschema": "^1.2.0",
    "nconf": "0.8.4"
  },
  "devDependencies": {
    "@iml/jasmine-n-matchers": "2.1.1",
    "@iml/req": "3.0.4",
    "babel-eslint": "7.2.3",
    "babel-jest": "^20.0.3",
    "babel-loader": "^7.1.2",
    "babel-plugin-external-helpers": "6.22.0",
    "babel-plugin-transform-flow-strip-types": "6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.0",
    "dotty": "0.0.2",
    "eslint": "^4.5.0",
    "eslint-config-prettier": "^2.3.0",
    "eslint-plugin-flowtype": "^2.35.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-prettier": "^2.2.0",
    "jasmine": "^2.7.0",
    "jest": "^19.0.2",
    "pre-commit": "1.2.2",
    "prettier": "^1.5.3",
    "webpack": "^3.5.5",
    "webpack-node-externals": "^1.6.0"
  }
}
