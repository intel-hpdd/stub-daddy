{
  "name": "@intel-js/stub-daddy",
  "version": "1.0.0",
  "description": "Stub API service used for integration testing.",
  "main": "index.js",
  "scripts": {
    "test": "mjnw",
    "integration": "mjnw test/integration/*-spec.js",
    "unit": "mjnw test/unit/*-spec.js",
    "cover": "cover",
    "jshint": "jshint --c ./.jshintrc --exclude-path ./.jshintignore .",
    "jscs": "jscs .",
    "ziplock": "../ziplocker/bin/ziplocker --overrides.ziplockDir='../../../../chroma-externals/'",
    "zipdev": "../ziplocker/bin/zip-install-dev --overrides.ziplockDir='../../../../chroma-externals/'",
  },
  "pre-commit": [
    "jscs",
    "jshint",
    "test"
  ],
  "repository" : {
    "type" : "git",
    "url" : "https://github.com/intel-js/fp.git"
  },
  "author": "IML Team",
  "license": "MIT",
  "dependencies": {
    "bunyan": "1.3.5",
    "configulator": "1.0.2",
    "jsonschema": "0.4.0",
    "wiretree": "0.2.5",
    "yargs": "1.2.6",
    "promise": "5.0.0",
    "@intel-js/lodash-mixins": "1.0.0"
  },
  "devDependencies": {
    "dotty": "0.0.2",
    "minijasminenodewrap": "1.0.1",
    "request-then": "0.0.1"
  }
}
